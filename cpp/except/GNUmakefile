6:
	@clear
	@cat 6noexcept_throw.cpp
	@g++ 6noexcept_throw.cpp -o 6noexcept_throw  -lstdc++ -std=c++17 -Wterminate
	@echo '********************* run ****************'
	@./6noexcept_throw || rm 6noexcept_throw
5:
	@clear
	@cat 5catch_except.cpp
	@g++ 5catch_except.cpp -o 5catch_except -lstdc++ -std=c++17
	@echo '********************* run ****************'
	@./5catch_except
	@rm 5catch_except 
4:	
	@clear
