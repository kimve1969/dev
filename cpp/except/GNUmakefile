	@clear
	@cat 3throw.cpp
	@g++ 3throw.cpp -o 3throw -lstdc++ -std=c++17
	@echo '********************* run ****************'
	@./3throw || rm 3throw
2:
	@clear
	@cat 2rollup_stack.cpp
	@g++ 2rollup_stack.cpp -o 2rollup_stack -lstdc++ -std=c++17
	@echo '********************* run ****************'
	@./2rollup_stack
